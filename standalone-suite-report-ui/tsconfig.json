{
  "compileOnSave": false,
  "compilerOptions": {
    "target": "es5",    
    "outDir": "./dist/out-tsc",
    "module": "commonjs",    
    "sourceMap": true,
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2015",
      "dom"
    ],
    "noImplicitAny": false,
    "suppressImplicitAnyIndexErrors": true        
  }
}
